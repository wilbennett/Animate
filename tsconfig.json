{
  "compilerOptions": {
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "outDir": "wwwroot/scripts",
    "target": "es2018",
    "module": "none"
  },
  "include": [
    "Client"
  ],
  "compileOnSave": true
}
